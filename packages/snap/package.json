{
  "name": "@metamask/ens-resolver-snap",
  "version": "0.1.0",
  "description": "A MetaMask Snap used for Ethereum Name Service (ENS) resolution",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/ens-resolver-snap.git"
  },
  "license": "Apache-2.0",
  "main": "./dist/bundle.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "build": "mm-snap build",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf dist",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "prepublishOnly": "mm-snap manifest",
    "serve": "mm-snap serve",
    "start": "mm-snap watch",
    "test": "jest"
  },
  "dependencies": {
    "@metamask/snaps-sdk": "^6.0.0",
    "ethers": "^6.13.1"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@metamask/auto-changelog": "3.4.4",
    "@metamask/eslint-config": "12.2.0",
    "@metamask/eslint-config-jest": "12.1.0",
    "@metamask/eslint-config-nodejs": "12.1.0",
    "@metamask/eslint-config-typescript": "12.1.0",
    "@metamask/snaps-cli": "6.2.1",
    "@metamask/snaps-jest": "8.2.0",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsdoc": "48.8.0",
    "eslint-plugin-n": "17.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "6.4.0",
    "jest": "29.5.0",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "29.1.0",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=18.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
