{
  "name": "root",
  "private": true,
  "description": "",
  "homepage": "https://github.com/MetaMask/ens-resolver-snap#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/ens-resolver-snap/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MetaMask/ens-resolver-snap.git"
  },
  "license": "Apache-2.0",
  "author": "",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --parallel --topological --verbose run build && yarn build-preinstalled-snap",
    "build-preinstalled-snap": "node scripts/build-preinstalled-snap.js",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,jsx,ts,tsx",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!**/CHANGELOG.md' '**/*.yml' --ignore-path .gitignore",
    "start": "yarn workspaces foreach --parallel --interlaced --verbose run start",
    "test": "yarn workspace @metamask/ens-resolver-snap run test"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "3.0.4",
    "@lavamoat/preinstall-always-fail": "2.0.0",
    "@metamask/eslint-config": "12.2.0",
    "@metamask/eslint-config-jest": "12.1.0",
    "@metamask/eslint-config-nodejs": "12.1.0",
    "@metamask/eslint-config-typescript": "12.1.0",
    "@metamask/snaps-controllers": "9.2.0",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsdoc": "48.8.0",
    "eslint-plugin-n": "17.9.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.4.0",
    "prettier": "2.8.8",
    "prettier-plugin-packagejson": "2.5.1",
    "sharp": "0.32.6",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=18.6.0"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "sharp": true
    }
  }
}
